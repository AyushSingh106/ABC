<%include ./partials/header.ejs%>
    <div class="w-full min-h-screen bg-cyan-700 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10">
            <% posts.forEach(function(elem) {%>
                <div class="cards w-1/6 h-[15vw]">
                    <div class="w-full h-full bg-cyan-800 rounded-lg  overflow-hidden">
                        <img class="h-full w-full object-center " src="/images/uploads/<%= elem.image%>" alt="">
                    </div>
                    <h5 class="mt-3 text-md">Title: "<%=elem.title%>"</h5>
                    <h6 class="mt-1 text-red-300 text-md"><%=elem. user.username%></h6>
                </div>
            <%})%>

        </div>
    </div>
    <%include ./partials/footer.ejs%>