<%include ./partials/header.ejs%>
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10">
            <%user.posts.forEach(function(elem) {%>
                <div class="cards w-1/4 h-[15vw]">
                    <div class="w-full h-full bg-sky-200 rounded-lg  overflow-hidden">
                        <img class="h-full w-full object-center " src="/images/uploads/<%= elem.image%>" alt="">
                    </div>
                    <h5 class="mt-5 text-md"><%=elem.title%></h5>
                </div>
            <%})%>

        </div>
    </div>
    <%include ./partials/footer.ejs%>